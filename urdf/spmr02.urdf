  <gazebo>
    <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
      <robotNamespace>/spmr02</robotNamespace>
    </plugin>

    <plugin name="differential_drive_controller" filename="libgazebo_ros_diff_drive.so">
      <alwaysOn>true</alwaysOn>
      <updateRate>100</updateRate>
      <rosDebugLevel>1</rosDebugLevel>
      <leftJoint>left_wheel_joint</leftJoint>
      <rightJoint>right_wheel_joint</rightJoint>
      <torque>500</torque>
      <commandTopic>spmr/cmd_val</commandTopic>
      <odometryTopic>/odom</odometryTopic>
      <odometryFrame>odom</odometryFrame>
      <publishTf>1</publishTf>
      <publishWheelTF>0</publishWheelTF>
      <publishOdomTF>1</publishOdomTF>
      <publishWheelJointState>1</publishWheelJointState>
      <wheelAcceleration>1</wheelAcceleration>
      <wheelTorque>500</wheelTorque>
      <odometrySource>world</odometrySource>
      <robotBaseFrame>base_link</robotBaseFrame>
      <wheelSeparation>0.8</wheelSeparation>
      <wheelDiameter>0.265</wheelDiameter>
    </plugin>
  </gazebo>

  <gazebo reference="RF_scan_link">
    <material value="Gazebo/Red" />
    <sensor type="ray" name="RF_scan">
      <always_on>true</always_on>
      <update_rate>15</update_rate>
      <pose>0.60802 -0.48579 -0.022036 0 0 0</pose>
      <!-- <pose>0 0 0 0 0 0</pose> -->
      <visualize>1</visualize>
      <ray>
        <!-- <pose>0 0 0 0 0 0</pose> -->
        <scan>
          <horizontal>
            <samples>271</samples>
            <resolution>1</resolution>
            <min_angle>-2.35619449019</min_angle>
            <max_angle>2.35619449019</max_angle>
          </horizontal>
        </scan>
        <range>
          <min>0.05</min>
          <max>15.0</max>
          <resolution>0.01</resolution>
        </range>
      </ray>
      <plugin name="gazebo_ros_RF_scan_controller" filename="libgazebo_ros_laser.so">
        <gaussianNoise>0.005</gaussianNoise>
        <alwaysOn>true</alwaysOn>
        <updateRate>15</updateRate>
        <topicName>RF_scan</topicName>
        <frameName>RF_scan</frameName>
      </plugin>
    </sensor>
  </gazebo>


  <gazebo reference="LR_scan_link">
